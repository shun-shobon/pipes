{
  "compilerOptions": {
    "lib": ["ESNext"],
    "module": "Node16",
    "target": "ESNext",
    "moduleResolution": "Node16",
    "moduleDetection": "force",
    "strict": true,
    "downlevelIteration": true,
    "skipLibCheck": true,
    "jsx": "preserve",
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "types": []
  }
}
